#***********accounts/services.py**************
#: accounts/services.py:36
msgid "The username can not be empty"
msgstr "El nombre de usuario no puede estar vacio"

#: accounts/services.py:38
msgid "The password can not be empty"
msgstr "La contraseña no puede estar vacia"

#: accounts/services.py:43
msgid "The user does not exist in the system"
msgstr "El usuario no existe en el sistema"

#: accounts/services.py:45
msgid "Account inactive, or your account is blocked"
msgstr "Cuenta inactiva, o su cuenta esta bloqueada"

#: accounts/services.py:48
msgid "Password is invalid, please enter your password correctly"
msgstr "Contraseña invalida, porfavor escriba correctamente su contraseña"

#: accounts/services.py:62
msgid "The access_token field can not be empty"
msgstr "El campo access_token no puede estar vacio"

#: accounts/services.py:64
msgid "The latitude field can not be empty"
msgstr "El campo latitud no puede estar vacio"

#: accounts/services.py:66
msgid "The field length can not be empty"
msgstr "El campo longitud no puede estar vacio"

#: accounts/services.py:80
msgid "The access token does not belong to that user"
msgstr "El token de acceso no pertenece a ese usuario"

#: accounts/services.py:88
msgid "The username exists, please enter another username"
msgstr "El nombre de usuario existe, porfavor escriba otro nombre de usuario"

#: accounts/services.py:95
msgid "The key that was used does not work"
msgstr "La llave que se utilzo no funciona"

#: accounts/services.py:102
msgid "The country is not registered in the system"
msgstr "El pais no esta registrado en el sistema"

#: accounts/services.py:166
msgid "In order to perform this operation, your account must be active"
msgstr "Para poder realizar esta operación su cuenta debe estar activa"

#: accounts/services.py:252
msgid "The old password field can not be empty"
msgstr "El campo vieja contraseña no puede esta vacio"

#: accounts/services.py:254
msgid "The new password field can not be empty"
msgstr "El campo nueva contraseña no puede esta vacio"

#: accounts/services.py:256
msgid "The password entered does not match your current password"
msgstr "La contraseña ingresada no coincide con tu actual contraseña"

#: accounts/services.py:258
msgid "The password must have characters and numbers"
msgstr "La contraseña debe tener caracteres y numeros"

#: accounts/services.py:280
msgid "The profile image field can not be empty"
msgstr "El campo imagen de perfil no puede estar vacio"

#: accounts/services.py:290
msgid "You can not upload more than 6 images in your profile, you must delete some"
msgstr "no puedes subir mas de 6 imagenes en tu perfil, debes eliminar alguna"

#: accounts/services.py:312
msgid "There is no such image on your profile"
msgstr "no existe esa imagen en tu perfil"

#: accounts/services.py:374
msgid "The distance field can not be empty"
msgstr "El campo distancia no puede estar vacio"

#: accounts/services.py:376
msgid "The distance value can only be numbers"
msgstr "el valor distancia solo puede ser numeros"

#: accounts/services.py:441
msgid "The public feed image field can not be empty"
msgstr "El campo imagen del feed publico no puede estar vacio"

#: accounts/services.py:466
msgid "An error occurred while saving the image"
msgstr "Ha ocurrido un error al guardar la imagen"

#: accounts/services.py:486
msgid "The image does not exist, you can not add a weed-like"
msgstr "La imagen no existe, no puedes agregarle un weed-like"

#: accounts/services.py:488
msgid "The like field can not be empty"
msgstr "El campo like no puede estar vacio"

#: accounts/services.py:505
msgid "Can not attach a weed-like to the image"
msgstr "No se puede anexar un weed-like a la imagen"

#: accounts/services.py:524
msgid "The image does not exist in your public feed or you have already deleted it"
msgstr "La imagen no existe en tu feed publico o ya la has eliminado"

#: accounts/services.py:551
msgid "The date field can not be empty"
msgstr "El campo de fecha no puede estar vacío"

#: accounts/services.py:553
msgid "The date you entered is not valid"
msgstr "La fecha que ingresaste no es válida"

#: accounts/services.py:558
msgid "The day is out of range per month"
msgstr "El día está fuera de rango por mes"

#: accounts/services.py:568
msgid "Please enter a valid email address"
msgstr "Por favor escriba una direccion de correo valida"

#: accounts/services.py:571
msgid "Mail exists, please enter another email"
msgstr "El correo existe, porfavor ingresa otro correo"

#: accounts/services.py:605
msgid "An error occurred while saving the user"
msgstr "Ha ocurrido un error al guardar el usuario"

#: accounts/services.py:629
msgid "The mail field can not be empty"
msgstr "El campo correo no puede estar vacio"

#: accounts/services.py:633
msgid "The mail is not registered in the system"
msgstr "El correo no esta registrado en el sistema"

#: accounts/services.py:651
msgid "The code field can not be empty"
msgstr "El campo codigo no puede estar vacio"

#: accounts/services.py:657
msgid "Code you sent does not match the one registered in your account"
msgstr "Código que enviaste no coinciden con el registrado en tu cuenta"
#***********accounts/services.py**************

#***********ceeberus/validations.py***********
msgid "empty values not allowed"
msgstr "valores vacíos no permitidos"

msgid "min length is 3"
msgstr "La longitud mínima es 3"

msgid "min length is 6"
msgstr "La longitud mínima es 6"

msgid "max length is 12"
msgstr "La longitud maxima es 12"

msgid "max length is 20"
msgstr "La longitud maxima es 20"

msgid "max length is 30"
msgstr "La longitud maxima es 30"

msgid "max length is 255"
msgstr "La longitud maxima es 255"

msgid "must be of string type"
msgstr "debe ser de tipo cadena"

msgid "unallowed value"
msgstr "valor no permitido "

msgid "must be of integer type"
msgstr "debe ser de tipo entero"

msgid "must be of date type"
msgstr "debe ser del tipo de fecha"
